        //cotacao field default value
        var d = new Date();

        var month = d.getMonth()+1;
        var day = d.getDate();
        var hours = d.getHours()
        var minutes = d.getMinutes()

        {% if quote is not defined %}
            var output = "Cotação " + ((''+day).length<2 ? '0' : '') + day + '/' +
                ((''+month).length<2 ? '0' : '') + month + ", " +
                hours + ":" + minutes;

            $("#cotacao").val(output);
        {% endif %}
