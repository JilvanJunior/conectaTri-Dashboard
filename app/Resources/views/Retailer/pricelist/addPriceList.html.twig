        function set2Digits(number) {
            let numberString = "" + number;

            if(numberString.length == 2)
                return numberString;
            else
                return "0" + numberString;
        }

        function saveQuote(type, force) {
            var name = $("#cotacao").val();
            var paymentDate = $('#prazoRCA').val();

            var startDate = $("#beginsAtTimePicker").data("datetimepicker").getDate();
            startDate = startDate.getFullYear() + "-" + set2Digits(startDate.getMonth() + 1) + "-" + set2Digits(startDate.getDate()) + "T"
                + set2Digits(startDate.getHours()) + ":" + set2Digits(startDate.getMinutes()) + ":00+00:00";

            var endDate = $("#expiresAtTimePicker").data("datetimepicker").getDate();
            endDate = endDate.getFullYear() + "-" + set2Digits(endDate.getMonth() + 1) + "-" + set2Digits(endDate.getDate()) + "T"
                + set2Digits(endDate.getHours()) + ":" + set2Digits(endDate.getMinutes()) + ":00+00:00";

            var productsId = [];
            var suppliersId = [];

            $.each($('#supplier-datatable tr input:checked'), function(i, v){
                if($(this).val() == 'on')
                    return true;

                suppliersId.push({
                    id:0,
                    representative: {
                        id: $(this).val()
                    },
                    quantity: 1,
                    price: 0
                });
            });

            console.log(suppliersId);

            $.each($('#product-datatable tr input:checked'), function(i, v){
                if($(this).val() == 'on')
                    return true;

                productsId.push({
                    product: {id: $(this).val()},
                    winners: [],
                    quote_suppliers: suppliersId
                });
            });

            $.ajax({
                url: "{{ quote is defined?path('put_api_quote', {id: quote.id}):path('post_api_quote') }}",
                type: '{{ quote is defined?'PUT':'POST' }}',
                contentType: "application/json",
                dataType: "json",
                headers: {'Api-Token': token},
                data:  JSON.stringify({
                    name: name,
                    type: type,
                    begins_at: startDate,
                    expires_at: endDate,
                    send_to_supplier: false,
                    payment_date: paymentDate,
                    quote_products: productsId,
                    quote_suppliers: suppliersId,
                }),
                success: function (response) {
                    if(force)
                        sendLink(response.id);
                    else
                        location.href = '{{ path('cotacoes') }}'
                },
                error: function (error) {
                    alert(error.responseJSON.error.exception["0"].message);

                }

            });
        }

        function sendLink(quoteId){

            var url = '{{ path("patch_api_send_quote", {'id': 0}) }}';
            url = url.replace(0, quoteId);
            $.ajax({
                url: url,
                type: 'PATCH',
                contentType: "application/json",
                dataType: "json",
                headers: {'Api-Token': token},
                success: function (response) {
                    location.href = '{{ path('cotacoes') }}'
                },
                error: function (error) {
                    alert(error.responseJSON.error.exception["0"].message);

                }

            });
        }

